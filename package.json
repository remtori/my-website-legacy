{
  "name": "my-website",
  "version": "0.0.9",
  "description": "Remtori's comfy home~",
  "main": "src/index.tsx",
  "scripts": {
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "start": "npm-run-all -p dev:*",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "npm run type-check && npm run lint",
    "build": "npm run build:webpack && npm run build:prerender",
    "build:webpack": "NODE_ENV=production webpack -p --config ./lib/webpack/webpack.client.js",
    "build:prerender": "NODE_ENV=production webpack -p --config ./lib/webpack/webpack.server.js",
    "build:netlify": "node ./lib/build.js",
    "dev:webpack": "webpack -d -w --config ./lib/webpack/webpack.client.js",
    "dev:server": "superstatic ./dist -p 8000 --host localhost --gzip -c '{\"rewrites\": [{\"source\":\"!/content/**/*\",\"destination\":\"index.html\"}]}'",
    "prod:server": "superstatic ./dist -p 8000 --host localhost --gzip -c '{\"cleanUrls\": true}'"
  },
  "repository": {
    "url": "https://github.com/remtori/my-website.git"
  },
  "author": "remtori <lqv.remtori@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/codemirror": "0.0.85",
    "@types/highlight.js": "^9.12.3",
    "@types/marked": "^0.7.3",
    "@types/yaml": "^1.2.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "extract-css-chunks-webpack-plugin": "^4.7.4",
    "file-loader": "^5.1.0",
    "fs-extra": "^8.1.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "schema-utils": "^2.6.5",
    "source-map-loader": "^0.2.4",
    "superstatic": "^6.0.4",
    "terser-webpack-plugin": "^2.3.5",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3",
    "undom": "^0.4.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "workerize-loader": "^1.1.0",
    "yaml": "^1.8.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "codemirror": "^5.52.0",
    "firebase": "^7.11.0",
    "flexsearch": "^0.6.32",
    "formik": "^2.1.4",
    "highlight.js": "^10.4.1",
    "marked": "^0.8.0",
    "preact": "^10.3.4",
    "preact-async-route": "^2.2.1",
    "preact-markup": "^2.0.0",
    "preact-router": "^3.2.1",
    "unistore": "^3.5.1"
  }
}
