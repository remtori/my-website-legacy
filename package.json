{
  "name": "my-website",
  "version": "1.0.0",
  "description": "Remtori's comfy home~",
  "main": "src/index.tsx",
  "scripts": {
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "start": "npm-run-all -p dev:*",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm-run-all build:*",
    "build:client": "cross-env NODE_ENV=production webpack -p --config ./libs/webpack/webpack.client.js",
    "build:prerender": "cross-env NODE_ENV=production webpack -p --config ./libs/webpack/webpack.server.js",
    "dev:webpack": "cross-env NODE_ENV=development webpack -d -w --config ./libs/webpack/webpack.client.js",
    "dev:server": "superstatic ./dist -p 8000 --host localhost --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=31536000\"}]}]}'",
    "firebase:deploy": "firebase deploy",
		"firebase:cors": "gsutil cors set cors.json gs://remtori.appspot.com"
  },
  "repository": {
    "url": "https://github.com/remtori/my-website.git"
  },
  "author": "remtori <lqv.remtori@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.8.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@types/highlight.js": "^9.12.3",
    "@types/marked": "^0.7.2",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "extract-css-chunks-webpack-plugin": "^4.7.1",
    "file-loader": "^5.0.2",
    "globby": "^11.0.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "schema-utils": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "superstatic": "^6.0.4",
    "terser-webpack-plugin": "^2.3.1",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.4",
    "undom": "^0.4.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "firebase": "^7.6.1",
    "formik": "^2.1.2",
    "highlight.js": "^9.17.1",
    "marked": "^0.8.0",
    "preact": "^10.0.5",
    "preact-async-route": "^2.2.1",
    "preact-router": "^3.1.0",
    "unistore": "^3.5.1"
  }
}
