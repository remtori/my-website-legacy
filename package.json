{
  "name": "my-website",
  "version": "1.0.0",
  "description": "Remtori's comfy home~",
  "main": "src/index.tsx",
  "scripts": {
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "start": "npm-run-all -p serve:webpack serve:sites",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "cross-env NODE_ENV=production webpack -p",
    "serve": "npm-run-all -p serve:webpack serve:firebase",
    "serve:webpack": "cross-env NODE_ENV=development webpack -d -w",
    "serve:sites": "firebase serve --only hosting:sites --port 8000",
    "serve:firebase": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"./service_cre.json\" && firebase serve",
    "deploy": "firebase deploy",
    "update-cors": "gsutil cors set cors.json gs://remtori.appspot.com"
  },
  "repository": {
    "url": "https://github.com/remtori/my-website.git"
  },
  "author": "remtori <lqv.remtori@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@types/highlight.js": "^9.12.3",
    "@types/marked": "^0.7.2",
    "@types/react-redux": "^7.1.5",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "extract-css-chunks-webpack-plugin": "^4.7.1",
    "file-loader": "^5.0.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prerender-loader": "^1.3.0",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.1",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
