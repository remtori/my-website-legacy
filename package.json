    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "firebase": "^7.6.1",
    "highlight.js": "^9.17.1",
    "ismobilejs": "^1.0.3",
    "marked": "^0.8.0",
    "preact": "^10.0.5",
    "preact-router": "^3.1.0"
  }
