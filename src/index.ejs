<!DOCTYPE html>
<html lang="en">
<head>
	<% const notMain = s => !/\/main.*\.(css|js)$/.test(s) %>

	<% for (var chunk in htmlWebpackPlugin.files.css) { %>
		<% if (notMain(htmlWebpackPlugin.files.css[chunk])) { %>
			<link rel="preload" href="<%= htmlWebpackPlugin.files.css[chunk] %>"  as="style">
		<% } %>
	<% } %>
	<% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
		<% if (notMain(htmlWebpackPlugin.files.chunks[chunk].entry)) { %>
			<link rel="preload" href="<%= htmlWebpackPlugin.files.chunks[chunk].entry %>" as="script">
		<% } %>
	<% } %>
	<title>Remtori's Comfy Home</title>
	<meta name="viewport" content="width=device-width">
	<meta name="theme-color" content="#13294D">
	<link rel="manifest" href="/manifest.json">
</head>
<body>
	<div id="app">{{prerender}}</div>
</html>
